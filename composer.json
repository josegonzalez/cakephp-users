{
    "name": "josegonzalez/cakephp-users",
    "description": "Users plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "admad/cakephp-social-auth": "0.3.*",
        "cakephp/cakephp": ">=3.5.0 <4.0.0",
        "josegonzalez/cakephp-upload": "3.*",
        "friendsofcake/crud": "5.*",
        "friendsofcake/crud-users": "0.*",
        "muffin/tokenize": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Users\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Users\\Test\\": "tests"
        }
    }
}
