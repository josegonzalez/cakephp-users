{
    "name": "josegonzalez/cakephp-users",
    "description": "Users plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "auth", "users"],
    "homepage": "https://github.com/josegonzalez/cakephp-users",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Diaz-Gonzalez",
            "email": "cakephp+users@josediazgonzalez.com",
            "homepage": "http://josediazgonzalez.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "admad/cakephp-social-auth": "0.6.*",
        "cakephp/cakephp": ">=3.5.0 <4.0.0",
        "josegonzalez/cakephp-upload": "4.*",
        "friendsofcake/crud": "5.*",
        "friendsofcake/crud-users": "0.*",
        "muffin/tokenize": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Users\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Users\\Test\\": "tests"
        }
    }
}
